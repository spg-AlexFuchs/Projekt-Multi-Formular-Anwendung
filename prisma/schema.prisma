datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Fahrrad {
  id    String @id @default(cuid())
  name  String
  preis Float
  zoll  Float
  farbe String
}

model Nutzer {
  id       String @id @default(cuid())
  vorname  String
  nachname String
  email    String @unique
  passwort String
}
